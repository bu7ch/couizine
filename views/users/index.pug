extends ../layout
block content
  h2 La liste des Utilisateurs 

  table
    thead
      tr
        th Nom
        th Email 
        th Code Postal 
    tbody
    each user in users 
      tr
        td
          a(href=`/users/${user._id}`)
            | #{user.fullname}
        td= user.email
        td= user.zipCode